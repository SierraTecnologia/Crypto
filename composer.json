{
    "name": "sierratecnologia/crypto",
    "description": "Cryptography tools for Laravel and Lumen",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "crypto"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/crypto/",
    "support": {
        "email": "help@sierratecnologia.com.br",
        "issues": "https://github.com/sierratecnologia/crypto/issues",
        "source": "https://github.com/sierratecnologia/crypto",
        "docs": "https://github.com/sierratecnologia/crypto/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        }
    ],
    "require": {
        "php": ">=5.0",
        "ext-gmp": "*",
        "defuse/php-encryption": "^2.2",
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0",
        "mdanter/ecc": "^0.5",
        "ramsey/uuid": "^3.8|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "mockery/mockery": ">=0.9.4",
        "mikey179/vfsstream": ">=1.6",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "SierraTecnologia\\Crypto\\": "src/"
        },
        "files": [
          "src/Helpers/crypto.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SierraTecnologia\\Crypto\\Test\\": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SierraTecnologia\\Crypto\\CryptoProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
